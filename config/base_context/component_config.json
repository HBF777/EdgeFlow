{
  "impl": ".non_separation.raspberry",
  "sensor": [
    {
      "name": "ExtTempHumSensor",
      "object": "TempHumSensor",
      "pin": {
        "temp": "A0",
        "hum": "A1"
      },
      "publish": "edge/sensor/ExtTempHumSensor/{device_id}",
      "data_format": {
        "temperature": "float",
        "humidity": "float"
      }
    },
    {
      "name": "ExtLightSensor",
      "object": "LightSensor",
      "pin": "A2",
      "publish": "edge/sensor/ExtLightSensor/{device_id}",
      "data_format": {
        "brightness": "float"
      }
    },
    {
      "name": "传感器name",
      "object": "传感器类",
      "pin": "引脚",
      "publish": "edge/sensor/ExtLightSensor/{device_id}发布主题",
      "data_format": {
        "brightness": "float返回格式"
      }
    },
    {
      "name": "传感器name",
      "object": "传感器类",
      "pin": "引脚",
      "publish": "edge/sensor/ExtLightSensor/{device_id}发布主题",
      "data_format": {
        "brightness": "float返回格式"
      }
    },
    {
      "name": "传感器name",
      "object": "传感器类",
      "pin": "引脚",
      "publish": "edge/sensor/ExtLightSensor/{device_id}发布主题",
      "data_format": {
        "brightness": "float返回格式"
      }
    },
    {
      "name": "传感器name",
      "object": "传感器类",
      "pin": "引脚",
      "publish": "edge/sensor/ExtLightSensor/{device_id}发布主题",
      "data_format": {
        "brightness": "float返回格式"
      }
    },
    {
      "name": "传感器name",
      "object": "传感器类",
      "pin": "引脚",
      "publish": "edge/sensor/ExtLightSensor/{device_id}发布主题",
      "data_format": {
        "brightness": "float返回格式"
      }
    }
  ],
  "lamp": [
    {
        "id": "LAMP{device_id}M",
        "name": "MasterLight",
        "object": "Lamp",
        "pin": "D2",
        "subscribe": "edge/lamp/Light/{lamp_id}",
        "data_format": {
          "brightness": "float"
        }
    },
    {
        "id": "LAMP{device_id}F",
        "name": "FrontLight",
        "object": "Lamp",
        "pin": "D3",
        "subscribe": "edge/lamp/Light/{lamp_id}",
        "data_format": {
          "brightness": "float"
        }
    },
    {
        "id": "LAMP{device_id}B",
        "name": "BackLight",
        "object": "Lamp",
        "pin": "D4",
        "subscribe": "edge/lamp/Light/{lamp_id}",
        "data_format": {
          "brightness": "float"
        }
    }
  ]
}